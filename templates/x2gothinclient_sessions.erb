# <%=  @managed_note %>
# <%=  @sessions %>
<%  @sessions.each do |session_id, content| %>

# values be changed via puppet by setting the corresponding hiera variable
# ID: <%= session_id %> content<%=  content %>
[<%= session_id %>]
name =<%= content['name'] %>
host = <%= content['host'] %>
command = <%= content['command'] %>
user = <%= content['user'] %>
rdpserver = <%= content['rdpserver'] %>
applications = <%= content['applications'] %>

# values that cannot (yet) be changed via puppet
defsndport = false
useiconv = false
iconvfrom = ISO8859-15
height = 768
export =
speed = 4
fullscreen = true
layout = us
width = 1024
quality = 9
xdmcpserver = localhost
soundtunnel = 1
rdpoptions =
soundsystem = pulse
print = 1
type = pc105/us
sndport = 4713
usekbd = true
fstunnel = true
link = ADSL
key =
startsoundsystem = false
icon =
sound = true
rootless = false
iconvto = UTF-8
useexports = true
dpi = 96
sshport = 22
setdpi = false
pack = 16m-jpeg
<% end %>
